<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <a href="/insertemp">Insert Employee</a>
    <table border="1">
        <thead>
            <tr>
                <th>id</th>
                <th>name</th>
                <th>email</th>
                <th>password</th>
                <th>department</th>
            </tr>
        </thead>
        <tbody>
            <%
                a.forEach(element => {%>
                    <tr>
                        <td><%= element._id %></td>
                        <td><%= element.name %></td>
                        <td><%= element.email %></td>
                        <td><%= element.password %></td>
                        <td><%= element.department %></td>
                    </tr>
                <%});
             %>
        </tbody>
    </table>
</body>
</html>